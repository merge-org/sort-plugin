{
    "name": "merge-org/sort",
    "version": "0.4.9-rc.1",
    "type": "wordpress-plugin",
    "autoload": {
        "MergeOrg\\Sort\\": "src",
        "Tests\\": "tests"
    },
    "require": {
        "php": "^7.4||^8.0||^8.1||^8.2||^8.3"
    },
    "require-dev": {
        "pestphp/pest": "^2",
        "phpstan/phpstan": "^1",
        "roave/backward-compatibility-check": "=8.7.0",
        "marcocesarato/php-conventional-changelog": "^1",
        "automattic/woocommerce": "^3",
        "wp-coding-standards/wpcs": "^3"
    },
    "autoload-dev": {
        "psr-4": {
            "MergeOrg\\Sort\\": "src",
            "Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}